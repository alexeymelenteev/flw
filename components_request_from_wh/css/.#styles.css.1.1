.grid{
	background-color:white;overflow:hidden;width:100%;height:450px;
}

.controller{
	float: left;
	width: 100px;
}
.controller .title{
	margin-left:15px;
	margin-right:15px;
	margin-top:4px;
}

.footer .row{
	float: left;
}

.message{
	border: 1px solid red;
	padding: 10px;
	color: red;
	width: 700px;
}

.inRowTitle{
	padding-right: 10px;
	font-size: 12px;
	font-weight: bold;
	float:left;
	width: 85px;
}

.rowInput{
	margin-bottom: 10px;
}

div.gridbox table.obj tr.rowselected td{
	background-color: #626E8F;
}

div.gridbox_light table.obj tr.rowselected td{
	background-color: #626E8F;
}
div.gridbox_light table.obj tr.rowselected td.cellselected, div.gridbox_light table.obj td.cellselected{
	background-color: #999FAF;
}